    skos:prefLabel "Actual heat-flow location"@en .

ihfc:literature a skos:Concept ;
    skos:definition "Specify the method in comments"@en ;
    skos:prefLabel "Literature/unspecified"@en .

ihfc:other a skos:Concept ;
    skos:definition "Specify the method in comments"@en ;
    skos:prefLabel "Other location"@en .

